<%= form_for(@journal) do |f| %>
    <%= f.label :title %>:<br>
    <%= f.text_area:title %><br/><br/>
    <%= f.label :amount %>:<br/>
    <%= f.number_field :amount %><br/><br/>
    <%= f.select :cash_inOut, options_for_select([['Cash In',true],['Cash Out',false]])%><br/><br>
    <%= f.select :user_id, options_for_select(User.all.map {|u| [u.full_name, u.id]}) %><br><br>
    <%= f.submit 'Create Journal',class:'btn btn-success' %>
<% end %>